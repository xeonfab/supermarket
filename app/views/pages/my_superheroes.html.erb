<div class="banner-home1">
    <nav class="nav1">
      <a class="nav1-link active" href="<%= account_my_superheroes_path %>">My Superheroes</a>
      <a class="nav1-link" href="<%= account_my_bookings_path %>">Bookings</a>
       <a class="nav1-link" href="<%= account_my_profile_path %>">Profile</a>
    </nav>
</div>
<div container-cards>
  <% @my_superheroes.each do |mysuperhero| %>
  <div class="card-superhero-account">
    <%= cl_image_tag mysuperhero.photo.key, alt: "alttext", class: "card-img-top"  %>
    <div class="middle">
      <h2><%= mysuperhero.name %></h2>
      <p><%= mysuperhero.description %></p>
      <p>Available</p>
    </div>
    <div class="last">
      <%= link_to "Edit", edit_superhero_path(mysuperhero), class: "btn btn-gradient" %>
      <%= link_to "Delete", superhero_path(mysuperhero), method: :delete, class: "btn btn-gradient" %>
      <p><%= mysuperhero.age %></p>
      <p><%= mysuperhero.superpower %></p>
       <p><%= mysuperhero.brains %></p>
        <p><%= mysuperhero.strength %></p>
         <p><%= mysuperhero.speed %></p>
          <p><%= mysuperhero.location %></p>
           <p><%= mysuperhero.price %></p>

    </div>
  </div>
  <% end %>
</div>
